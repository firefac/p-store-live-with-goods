.before-detail {
  height: 80rpx;
  line-height: 80rpx;
  display: block;     /*设置为块级元素会独占一行形成上下居中的效果*/
  text-align: center;
  position: relative;   /*定位横线（当横线的父元素）*/
  background-color: #ffffff;
  margin-top: 15rpx;
}
.before-detail:before, .before-detail:after {
  content: '';                 /*CSS伪类用法*/
  position: absolute;         /*定位背景横线的位置*/
  top: 45%;
  background: #CCCCCC;       /*宽和高做出来的背景横线*/
  width: 60rpx;
  height: 2rpx;
}
.before-detail:before{
  left: 250rpx;        /*调整背景横线的左右距离*/
}
.before-detail:after {
  right: 250rpx;
}

