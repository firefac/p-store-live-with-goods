.container {
  margin-bottom: 100rpx;
  background-color: #F5F5F5;
}

.post{
  position: relative;
}

.post .liveimgs {
  width: 750rpx;
  height: 750rpx;
}

.post .liveimgs image {
  width: 750rpx;
  height: 750rpx;
}


.container .post .label {
  height: 50rpx;
  width: 50rpx;
  border-radius: 50%;
  text-align: center;
  color: #fff;
  position: absolute;
  top: 15rpx;
  left: 15rpx;
  font-size: 28rpx;
  background-size: 55rpx auto;
  padding: 10rpx;
  background-color: #669933;
}

.container .post .top {
  height: 35rpx;
  width: 80rpx;
  border-radius: 25rpx;
  background-color: #FF6600;
  text-align: center;
  color: #fff;
  position: absolute;
  bottom: 15rpx;
  left: 15rpx;
  font-size: 24rpx;
  background-size: 55rpx auto;
}

.container .post .desc {
  width:200rpx;
  height: 50rpx;
  max-width: 400rpx;
  border-radius: 25rpx;
  background-color: rgba(0,0,0,0.3);
  text-align: center;
  color: #fff;
  position: absolute;
  top: 15rpx;
  left: 15rpx;
  font-size: 28rpx;
  background-size: 55rpx auto;
  padding-left: 50rpx;
}

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 20rpx;
}

.live-info {
  width: 750rpx;
  min-height: 100rpx;
  overflow: hidden;
  background: #fff;
  padding-top: 30rpx;
}

.live-info .c {
  display: block;
  width: 718.75rpx;
  height: 100%;
  margin-left: 30rpx;
}

.live-info .c text {
  display: block;
  width: 687.5rpx;
  text-align: left;
}

.live_name {
  /* border: 1px solid black; */
  height: auto;
  background-color: #fff;
  padding: 0rpx 0rpx 10rpx 0rpx;
}

.live_name_left {
  /* border: 1px solid #757575; */
  float: left;
  height: auto;
  font-weight: 550;
  line-height: 40rpx;
  font-size: 30rpx;
  letter-spacing: 1rpx;
  max-width: 610rpx;
}

.live_name_right {
  float: right;
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding: 0;
  margin: 0;
  margin-right: 0rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  border-top-right-radius: 0rpx;
  border-bottom-right-radius: 0rpx;
  letter-spacing: 3rpx;
  background-color: #a78845;
  /* background-image: linear-gradient(to right, #ff7701 100%); 
  background-image: linear-gradient(to right, #9a9ba1 0%, #9a9ba1 100%);*/
}

.live-info .desc {
  height: auto;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  line-height: 33rpx;
  color: #999;
}

.live-info .status {
  display: flex;
  margin-bottom: 20rpx;
  margin-right: 30rpx;
  line-height: 33rpx;
}

.live-info .status .txt {
  width: 50%;
  height: auto;
  font-size: 32rpx;
  color: #669933;
}

.live-info .status .datetime {
  width: 50%;
  text-align: right;
  height: auto;
  font-size: 24rpx;
  color: #999;
}

.detail {
  width: 750rpx;
  height: auto;
  overflow: hidden;
  padding-top: 15rpx;
  background-color: #ffffff;
}

.detail image {
  width: 750rpx;
  display: block;
}

.related-goods {
  width: 750rpx;
  height: auto;
  overflow: hidden;
  background-color: #fff;
}

.related-goods .h {
  position: relative;
  height: 145.5rpx;
  width: 750rpx;
  padding: 56.25rpx 0;
  background: #fff;
  text-align: center;
}

.related-goods .h .line {
  display: inline-block;
  position: absolute;
  top: 72rpx;
  left: 0;
  z-index: 2;
  height: 1px;
  margin-left: 225rpx;
  width: 300rpx;
  background: #ccc;
}

.related-goods .h .title {
  display: inline-block;
  position: absolute;
  top: 56.125rpx;
  left: 0;
  z-index: 3;
  height: 33rpx;
  margin-left: 285rpx;
  width: 180rpx;
  background: #fff;
}

.related-goods .b {
  width: 750rpx;
  height: auto;
  overflow: hidden;
  margin-bottom: 20rpx;
}

.related-goods .b .item {
  float: left;
  background: #fff;
  width: 375rpx;
  height: auto;
  overflow: hidden;
  text-align: center;
  padding: 15rpx 31.25rpx;
}

.related-goods .item .img {
  width: 311.45rpx;
  height: 311.45rpx;
}

.related-goods .item .name {
  display: block;
  width: 311.45rpx;
  height: 70rpx;
  margin: 11.5rpx 0 5rpx 0;
  text-align: left;
  overflow: hidden;
  font-size: 26rpx;
  color: #333;
}

.related-goods .item .desc {
  display: block;
  width: 311.45rpx;
  overflow: hidden;
  height: 35rpx;
  margin: 0 0 15rpx 0;
  text-align: left;
  font-size: 28rpx;
  color: #666;
}

.related-goods .item .price {
  display: block;
  width: 311.45rpx;
  height: 30rpx;
  text-align: left;
  font-size: 26rpx;
  color: #993333;
}

.bottom-btn {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 10;
  width: 750rpx;
  height: 100rpx;
  display: flex;
  background: #fff;
}

.bottom-btn .c {
  background: #b4282d;
